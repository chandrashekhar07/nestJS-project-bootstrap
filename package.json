{
  "name": "project-name",
  "version": "0.0.1",
  "description": "NestJS API project ",
  "author": "Chandra shekhar neupane",
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "start": "node main.js",
    "build": "npm run clean && tsc",
    "test": "env-cmd -f .env jest --config=jest.json --notify",
    "migration:create": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:create -n migration",
    "migration:generate": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:generate -n migration",
    "migration:run": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:revert",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "check-types": "tsc --pretty --noEmit",
    "prepare": "husky install",
    "docs": "npx @compodoc/compodoc -p tsconfig.json -s",
    "dev": "env-cmd -f .dev.env nest start --watch",
    "staging": "env-cmd -f .staging.env nest start --watch",
    "local": "env-cmd -f .local.env nest start --watch",
    "prod": "env-cmd -f .prod.env nest start --watch",
    "repl:local": "npm run start:dev -- --watch --entryFile repl",
    "repl:dev": "npm run dev -- --watch --entryFile repl",
    "repl:staging": "npm run staging -- --watch --entryFile repl",
    "repl:prod": "npm run prod -- --watch --entryFile repl",
    "commitlint": "commitlint --edit",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.150.0",
    "@nestjs/common": "^9.0.9",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.9",
    "@nestjs/platform-express": "^9.0.11",
    "@nestjs/swagger": "^6.0.5",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/typeorm": "^9.0.1",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "express": "^4.17.1",
    "helmet": "^5.1.1",
    "husky": "^8.0.0",
    "lodash": "^4.17.21",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.7",
    "winston": "^3.3.3",
    "winston-cloudwatch": "^6.1.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.19",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.9",
    "@types/body-parser": "~1.19.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^28.1.8",
    "@types/lodash": "^4.14.183",
    "@types/node": "^18.7.6",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "env-cmd": "^10.0.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-filenames-simple": "^0.9.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^51.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^15.2.2",
    "prettier": "^2.3.0",
    "supertest": "^6.1.3",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.2.4"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}